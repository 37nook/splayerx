{
  "appId": "org.splayer.splayerx",
  "productName": "SPlayer",
  "publish": [
    {
      "provider": "github",
      "owner": "chiflix",
      "repo": "splayerx"
    }
  ],
  "afterPack": "scripts/after-pack.js",
  "compression": "maximum",
  "directories": {
    "output": "build"
  },
  "electronVersion": "5.0.11",
  "electronDist": "node_modules/@chiflix/electron/dist",
